dim     0.1;
mid     #calc "$dim/2.0";
size    60;

dirs    0.2;
dirc    #calc "1-2*$dirs";
cells   0.3;
cellc   #calc "1-2*$cells";
exl     4.0;
exr     #calc "1.0/$exl";

simTime 5;
dt      0.0005;

vel     1;
Re      5000;

