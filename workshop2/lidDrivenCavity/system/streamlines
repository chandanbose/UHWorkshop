#include "${FOAM_CASE}/system/blockMeshDict"

nLines  $size;
start   (#calc "$dim/2.0" 0 0);
end     (#calc "$dim/2.0" #calc "$dim/2.0" 0);
fields  (U);

#includeEtc "caseDicts/postProcessing/visualization/streamlines.cfg"

