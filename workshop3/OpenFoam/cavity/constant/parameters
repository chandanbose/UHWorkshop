FoamFile
{
    version     2.0;
    format      ascii;
    class       IOobject;
    location    "constant";
    object      parameters;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
// Mesh parameters
dim     1.0;
mid     0.5;
size    128;

// Simulation parameters
simTime 90.0;
dt      1e-4;
tol     1e-8;

// Physical parameters
vel     1.0;
re      1000.0;
nu      0.001;
c2      5.8;

