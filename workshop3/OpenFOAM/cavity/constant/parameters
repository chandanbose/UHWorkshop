FoamFile
{
    version     2.0;
    format      ascii;
    class       IOobject;
    location    "constant";
    object      parameters;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
// Mesh parameters
dim     1.0;
mid     0.5;
size    64;

// Simulation parameters
simTime 70.0;
dt      1e-3;
tol     1e-6;

// Physical parameters
vel     1.0;
re      5000.0;
nu      0.0002;
c2      5.8;

